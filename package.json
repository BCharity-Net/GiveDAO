  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "env-cmd": "^10.1.0",
    "postcss-preset-env": "^7.4.3",
    "source-map-loader": "^3.0.1",
