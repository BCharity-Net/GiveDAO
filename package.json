{
  "name": "givedao-profile",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "description": "ts examples in how to call the Lens API",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
