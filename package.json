    "graphql": "^16.3.0",
    "ipfs-http-client": "^56.0.1",
    "next": "^12.1.0",
    "omit-deep": "^0.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.70.0"
